{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Start React Server",
            "type": "shell",
            "command": "cd /workspaces/LandInsights/react_app && npm start",
            "isBackground": true,
            "problemMatcher": {
                "owner": "custom",
                "pattern": {
                    "regexp": "^$",
                    "file": 1,
                    "location": 2,
                    "message": 3
                },
                "background": {
                    "activeOnStart": true,
                    "beginsPattern": "^.*Compiled successfully.*$",
                    "endsPattern": "^.*Failed to compile.*$"
                }
            },
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}
